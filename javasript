            var b=parseInt(prompt("enter 2nd:"));
             var c=parseInt(prompt("enter 3rd:"));
             if(a>b)
             {
                 if(a>c)
                 {
                     document.write(a);
                 }
                 else
                 {
                     document.write(c);
                 }
             }
             else
             {
                 if(b>c)
                 {
                     document.write(b);
                 }
                 else
                 {
                     document.write(c);
                 }
             }
           
        </script>
